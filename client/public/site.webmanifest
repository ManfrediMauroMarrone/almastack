    "handle_links": "preferred",

    "file_handlers": [],

    "protocol_handlers": [],

    "share_target": {
        "action": "/share",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
